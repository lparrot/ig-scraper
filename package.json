{
  "name": "ig-webscraping",
  "productName": "Instant Gaming Web Scraping",
  "version": "1.0.3",
  "description": "Application de webscraping pour Instant Gaming",
  "main": ".vite/build/main.js",
  "type": "commonjs",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "build": "electron-forge package && electron-builder --win --publish never",
    "publish": "electron-forge package && electron-builder --win --publish always",
    "publish:patch": "npm version patch --force -m \"Mise à jour version %s.\"",
    "publish:minor": "npm version minor --force -m \"Mise à jour version %s.\"",
    "publish:major": "npm version major --force -m \"Mise à jour version %s.\"",
    "postversion": "git push && git push --tags"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/lparrot/ig-webscraping"
  },
  "author": {
    "name": "laurent.parrot",
    "email": "laurent.parrot@def.gouv.fr"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.2",
    "@electron-forge/plugin-vite": "^7.8.2",
    "@types/cheerio": "^0.22.35",
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "37.2.5",
    "electron-builder": "^26.0.12",
    "typescript": "~5.9.2",
    "vite": "^5.4.19"
  },
  "dependencies": {
    "@nuxt/ui": "^3.3.0",
    "cheerio": "^1.1.2",
    "dexie": "^4.2.0",
    "electron-updater": "^6.6.2",
    "pinia": "^3.0.3",
    "update-electron-app": "^3.1.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "zod": "^4.0.17"
  }
}
